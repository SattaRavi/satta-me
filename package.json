{
  "name": "satta-me",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "npx del dist",
    "debug": "DEBUG=* npx eleventy",
    "watch:html": "ELEVENTY_ENV=dev npx eleventy --serve",
    "watch:css": "npx node-sass -w ./src/layouts -o ./src/assets",
    "watch": "npm-run-all --parallel watch:css watch:html",
    "start": "npm run watch",
    "build:css": "npx node-sass ./src/layouts -o ./src/assets --output-style compressed",
    "build:html": "ELEVENTY_ENV=prod npx eleventy",
    "build": "npm-run-all build:css build:html"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.10.0",
    "del-cli": "^3.0.0",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SattaRavi/satta-me.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SattaRavi/satta-me/issues"
  },
  "homepage": "https://github.com/SattaRavi/satta-me#readme"
}
